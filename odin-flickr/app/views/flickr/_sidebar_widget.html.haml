.flickr
  %ul
    - photos.each do |row|
      %li
        - row.each do |p|
          - unless p.nil?
            - info = flickr.photos.getInfo(photo_id: p.id)
            = link_to(image_tag(FlickRaw.url_s(info), :class => 'flickr_photo', :title => p.title, :border => 0, :size => '75x75'), FlickRaw.url_photopage(info))